<%= provide(:title, "Contact") %>

<h1>Contact Us</h1>
<div class="row">
  <div class="span5">
    <%= form_for(@contact) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
     
      <%= f.label :name %>
      <%= f.text_field :name %>
     
      <%= f.label :email %>
      <%= f.text_field :email %>
     
      <%= f.label :message %>
      <%= f.text_area :message %>
     
      <%= f.submit "Contact" , class:"btn btn-primary" %>
    <% end %>
  </div>
</div>
